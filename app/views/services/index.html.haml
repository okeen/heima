%h1= t "services.title"

- services_list =  ["imagen_corporativa", "elaboracion_listados", "confirmacion_asistentes", "mailing", "secretaria_tecnica", "protocolo_evento", "azafatas_congresos_imagen", "medios_audiovisuales", "gabinete_prensa", "transfers", "montaje_stands", "traductores", "viajes_alojamiento"]

#services_container.big_panel
  %ul#services_list.left_panel
